#include<stdio.h>
int main()
{
	int d1;
	printf("enter the decimal number");
	if(scanf("%d",&d1))
	{
		printf("\n decimal number -->%d",d1);
		printf("\n octal number-->%o ",d1);
	}
	else
	{
		printf("invalid");
	}
}
